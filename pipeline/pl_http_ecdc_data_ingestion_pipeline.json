{
	"name": "pl_http_ecdc_data_ingestion_pipeline",
	"properties": {
		"activities": [
			{
				"name": "Copy ecdc death case https data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "source_url",
						"value": "cloudboxacademy/covid19/main/ecdc_data/hospital_admissions.csv"
					},
					{
						"name": "sink_filename",
						"value": "hospital_admissions.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_ecdc_data_case_death_http",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_death_case_http_dl",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}