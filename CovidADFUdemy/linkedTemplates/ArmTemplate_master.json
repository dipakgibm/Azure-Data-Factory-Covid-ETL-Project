{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CovidADFUdemy"
		},
		"ls_ablob_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_sa'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://coviddeltalakesa.dfs.core.windows.net/"
		},
		"population_sa_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/ec2120be-b93a-48a5-aa7f-33543b17f6c0/resourceGroups/CovidProject/providers/Microsoft.Storage/storageAccounts/covidpsa"
		},
		"ls_ecdc_cases_death_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_source_url": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/main/ecdc_data/cases_deaths.csv"
		},
		"trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_sink_filename": {
			"type": "string",
			"defaultValue": "cases_deaths.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CovidADFUdemy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_sa_connectionString": {
						"value": "[parameters('ls_ablob_sa_connectionString')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"ls_ecdc_cases_death_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_cases_death_http_properties_typeProperties_url')]"
					},
					"population_sa_trigger_properties_typeProperties_scope": {
						"value": "[parameters('population_sa_trigger_properties_typeProperties_scope')]"
					},
					"trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_source_url": {
						"value": "[parameters('trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_source_url')]"
					},
					"trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_sink_filename": {
						"value": "[parameters('trigger_ingest_hospital_data_properties_pl_http_ecdc_data_ingestion_pipeline_parameters_sink_filename')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}