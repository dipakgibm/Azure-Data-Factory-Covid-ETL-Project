			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CovidADFUdemy"
		},
		"ls_ablob_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_sa'"
